{
    "name": "rnr/laravel-logger-mock",
    "description": "Mock for laravel logger interface to handle log messages in test",
    "type": "library",
    "version": "0.1",
    "require": {
        "laravel/framework": "~5.1",
        "friendsofphp/php-cs-fixer": "^1.12"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~5.6"
    },
    "autoload": {
        "psr-4": {
            "Rnr\\LoggerMock\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rnr\\Tests\\LoggerMock\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Sergei Melnikov",
            "email": "me@rnr.name"
        }
    ]
}
